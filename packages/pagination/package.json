{
  "name": "@unstyled-react/pagination",
  "version": "0.0.2",
  "description": "Unstyled pagination component for React",
  "keywords": [
    "react",
    "component",
    "paginate",
    "paginator",
    "pagination"
  ],
  "author": "Ugochukwu Nneji <hi@ugee.xyz>",
  "homepage": "https://github.com/thisugee/unstyled-react#readme",
  "license": "MIT",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "files": [
    "build"
